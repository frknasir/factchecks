{
  "name": "starfolksoftware/factchecks",
  "description": "Add factchecking to your Laravel application",
  "keywords": [
    "starfolk",
    "starfolksoftware",
    "factchecks"
  ],
  "homepage": "https://github.com/starfolksoftware/factchecks",
  "license": "MIT",
  "authors": [
    {
      "name": "Faruk Nasir",
      "email": "frknasir@yahoo.com",
      "homepage": "https://faruknasir.com",
      "role": "Co-Founder Starfolk Software"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^3.6|^5.0",
    "phpunit/phpunit": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "StarfolkSoftware\\Factchecks\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StarfolkSoftware\\Factchecks\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "StarfolkSoftware\\Factchecks\\CommentsServiceProvider"
      ]
    }
  }
}
